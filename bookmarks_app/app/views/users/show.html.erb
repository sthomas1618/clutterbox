<div id="user_feed" class="center">
  <h1>
    <%= @user.username %>
  </h1>
  <dl id="profile">
    <dt>
       Name
    </dt>
    <dd>
      <%= @user.first_name + " " + @user.last_name %>
    </dd>
    <dt>
      Email
    </dt>
    <dd>
      <%= @user.email %>
    </dd>
  </dl>
  <div class="follow">
    <a href="joe_user/followers.html" > blank Followers </a>
    <a href="joe_user/followings.html" > Following blank </a>
  </div>
  <h4 class="createB">
    <%= link_to "New Bookmark", new_bookmark_path %>
  </h4>
  <section id="bookmarks" class="feed">
    <%= render @bookmarks %>
  </section>
</div>